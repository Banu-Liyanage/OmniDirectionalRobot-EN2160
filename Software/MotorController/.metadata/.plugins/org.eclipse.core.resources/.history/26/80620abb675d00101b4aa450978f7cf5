#include "motion.h"


void Motion_Init(Motion *motion, Controller *controller, Profile *x_profile, Profile *y_profile, Profile *W_profile) {
    motion->controller = *controller;
    motion->forward = *forward;
    motion->rotation = *rotation;
}
